<%# cart show page%>
<section class="cart-show">

  <header class="page-header">
    <h1>My Cart</h1>
  </header>

  <%# renders cart review page if cart not empty, or the 'empty cart' message page %>
  <%= render 'cart_review', cart: cart unless cart.size == 0 %>
  <%= render 'empty_cart_message' if cart.size == 0 %>

</section>
